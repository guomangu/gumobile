cd app && composer install
cd ../
./frankenphp-linux-x86_64 php-cli app/bin/console tailwind:build --watch -v
chmod +x app/var/tailwind/v4.1.11/tailwindcss-linux-x64
rm app/var/tailwind/v4.1.11/tailwindcss-linux-x64
./frankenphp-linux-x86_64 php-cli app/bin/console tailwind:build --watch -v
./frankenphp-linux-x86_64 php-server --listen :8080 -r app/public/